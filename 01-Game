#include <iostream>
#include <vector>
#include<algorithm>
using namespace std;

int main() {
   int t;
   cin>>t;
   while(t--){
    string s;
    cin>>s;
    int n=s.size();
    int c1=0;
    int c2=0;
    for(int i=0;i<n;i++){
        if(s[i]=='1'){c1++;}
        else{c2++;}
    }
    int c=min(c1,c2);
    if(c%2!=0){
       cout<<"DA"<<endl;
    }
    else{
        cout<<"NET"<<endl;
    }
}
   return 0;
}
