#include <bits/stdc++.h>
using namespace std;

int main() {
    long long t;
    cin >> t;
    while (t--) {
        long long n,k;
        cin>>n>>k;
        string s;
        cin>>s;
        unordered_map<char,long long>mp;
        for(int i=0;i<n;i++){
            mp[s[i]]++;
        }
        long long cnt=0;
        for(auto it:mp){
            if((it.second)%2!=0){
                cnt++;
            }
        }
        if(cnt-1<=k){
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
    }
    return 0;
}
