#include <bits/stdc++.h>
using namespace std;

int main() {
    long long t;
    cin >> t;
    while (t--) {
        long long n,k,x;
        cin>>n>>k>>x;
        long long minsum=k*(k+1)/2;
        long long maxsum=n*k-k*(k-1)/2;
        if(x<=maxsum && x>=minsum){
            cout<<"YES"<<endl;
        
        }
        else {
            cout<<"NO"<<endl;
        }
    }
    return 0;
}
