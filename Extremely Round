#include <iostream>
#include <vector>
using namespace std;

int main() {
    long long t;
    cin >> t;
    while (t--) {
        int n;
        cin>>n;
        if(n<=9)cout<<n<<endl;
        else if(n<=99)cout<<n/10+9<<endl;
        else if(n<=999)cout<<n/100+9+9<<endl;
        else if(n<=9999)cout<<n/1000+9+9+9<<endl;
        else if(n<=99999)cout<<n/10000+9+9+9+9<<endl;
        else cout<<n/100000+9+9+9+9+9<<endl;
    }
    return 0;
}
