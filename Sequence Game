#include <iostream>
#include <vector>
using namespace std;
 
int main() {
    long long t;
    cin >> t;
    while (t--) {
      long long n;
      cin>>n;
      vector<int>b(n);
      bool found=false;
      for(int i=0;i<n;i++){
        cin>>b[i];
      }
      vector<int>a;
      a.push_back(b[0]);
      for(int i=1;i<n;i++){
        if(b[i-1]>b[i]){
            a.push_back(b[i]);
        }
            a.push_back(b[i]);
      }
      cout<<a.size()<<endl;
      for(int i=0;i<a.size();i++){
        cout<<a[i]<<" ";
      }
    }
return 0;}
