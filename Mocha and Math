#include<iostream>
#include<vector>
using namespace std;
int main(){
   int t;
   cin>>t;
   while(t--){
    int n;
    cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int s=a[0];
    for(int i=1;i<n;i++){
        s = s & a[i];
    }
    cout<<s<<endl;
   }
    return 0;
}
