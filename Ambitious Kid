#include <iostream>
#include <vector>
#include<algorithm>
#include<climits>
#include <cmath>
using namespace std;

int main() {
    long long n;
    cin>>n;
    vector<long long>arr(n);
    long long diff=INT_MAX;
    for(int i=0;i<n;i++){
        cin>>arr[i];
        if(arr[i]<0){
            diff=min(diff,abs(arr[i]));
        }
        else{
            diff=min(diff,arr[i]);
        }
    }
    cout<<diff<<endl;
    return 0;
}
