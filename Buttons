#include<iostream>
#include<vector>
#include<algorithm>
#include<unordered_map>
using namespace std;

int main(){
    long long t;
    cin>>t;
    while(t--){
        long long a,b,c;
        cin>>a>>b>>c;
        if(a==b){
            if(c%2==0)cout<<"Second"<<endl;
            else cout<<"First"<<endl;
        }
        else if(a<b){
            if(a+c<b)cout<<"Second"<<endl;
            else if((c-(b-a))%2==0)cout<<"Second"<<endl;
            else cout<<"First"<<endl;
        }
        else {
            if(b+c<a)cout<<"First"<<endl;
            else if((c-(a-b))%2==0)cout<<"Second"<<endl;
            else cout<<"First"<<endl;
        }
    }
    return 0;
}
