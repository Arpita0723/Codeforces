#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
using namespace std;
int main(){
   long long t;
   cin>>t;
   while(t--){
    long long a,b,c;
    cin>>a>>b>>c;
    bool ok=false;
    long long n_a=2*b-c;
    if(n_a>0 && n_a%a==0){
        ok=true;
    }
    long long sum_ac=a+c;
    if(sum_ac%2==0){
        long long n_b=a+c/2;
        if(n_b>0 && n_b%b==0){
            ok=true;
        }
    }
    long long n_c=2*b-a;
    if(n_c>0 && n_c%c==0){
        ok=true;
    }
    if(ok){cout<<"YES"<<endl;}
    else{
        cout<<"NO"<<endl;
    }
}
    return 0;
}
